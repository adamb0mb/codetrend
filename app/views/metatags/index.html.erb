<h1>Listing metatags</h1>

<table>
  <tr>
    <th>Tag</th>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @metatags.each do |metatag| %>
  <tr>
    <td><%= metatag.tag %></td>
    <td><%= metatag.title %></td>
    <td><%= link_to 'Show', metatag %></td>
    <td><%= link_to 'Edit', edit_metatag_path(metatag) %></td>
    <td><%= link_to 'Destroy', metatag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Metatag', new_metatag_path %>
